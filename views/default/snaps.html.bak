{{extend 'layout.html'}}
<!-- New ratings interface -->
{{if dh == 'cowell':}}
    <h1>Cowell/Stevenson Snaps</h1>
{{elif dh == 'crown':}}
    <h1>Crown/Merrill Snaps</h1>
{{elif dh == 'porter':}}
    <h1>Porter/Kresge Snaps</h1>
{{elif dh == 'eight':}}
    <h1>Eight/Oakes Snaps</h1>
{{elif dh == 'nine':}}
    <h1>Nine/Ten Snaps</h1>
{{pass}}
{{=A('Back to Dining Hall Selection', _class = 'btn btn-primary', _href = URL('menu', args = dh))}}
<br />
{{for snap in snaps:}}
    <div class = 'well'>
        <h4>{{=snap.user}} &bull; {{=snap.date.strftime('%b %d')}}</h4>
        {{=snap.file}}
    </div>
{{pass}}
